<idea-plugin>
    <id>com.github.ofofs.jca</id>
    <name>Java Compile Annotation Plugin</name>
    <version>1.0</version>
    <vendor email="java@kangyonggan.com" url="https://github.com/ofofs">ofofs</vendor>

    <description><![CDATA[
      Java Compile Annotation Plugin.<br>
    ]]></description>

    <change-notes><![CDATA[
      Plugin Init.<br>
    ]]>
    </change-notes>

    <idea-version since-build="145.0"/>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>

    <actions>
        <group id="JcaActionGroup" text="Jca" description="Refactor code with jca" popup="true">
            <action id="defaultGetter" class="com.github.ofofs.jca.action.GetterAction"
                    text="Default @Getter"
                    description="Action to replace all getter methods with Java Compile @Getter annotation">
            </action>
            <add-to-group group-id="RefactoringMenu" anchor="last"/>
        </group>
    </actions>

</idea-plugin>